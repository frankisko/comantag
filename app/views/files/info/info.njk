{% extends "layouts/base/base.njk" %}

{% block content %}
    <!-- Page Heading -->
    <h1 class="h3 mb-2 text-gray-800">Info</h1>

    <div class="row">
        <div class="col-lg-6">
            <div class="card shadow mb-4">
                <div class="card-header py-3">
                    <h6 class="m-0 font-weight-bold text-primary">
                        <div class="text-left">
                            <span><a href="#">File</a></span>
                        </div>
                    </h6>
                </div>
                <div class="card-body">
                    <div class="row">
                        <div class="col-lg-4">
                            {% if row.scrapped %}
                                <open-file-viewer
                                    thumbnail-url="/collections/{{ row.id_collection }}/thumbnails/{{ row.id_file }}.jpg"
                                    id-file="{{ row.id_file }}"
                                    file="{{ row.full_path }}">
                                </open-file-viewer>
                            {% else %}
                                <img src="/images/no_preview.jpg" class="card-img-top">
                            {% endif %}
                        </div>
                        <div class="col-lg-8">
                            <table class="table">
                                <tr>
                                    <td>Id</td>
                                    <td>{{ row.id_file }}</td>
                                </tr>
                                <tr>
                                    <td>Path</td>
                                    <td>{{ row.path }}</td>
                                </tr>
                                <tr>
                                    <td>Filename</td>
                                    <td>{{ row.filename }}</td>
                                </tr>
                                <tr>
                                    <td>Size</td>
                                    <td>{{ row.human_size }}</td>
                                </tr>
                                <tr>
                                    <td>Pages</td>
                                    <td>{{ row.pages }}</td>
                                </tr>
                                <tr>
                                    <td>Created at</td>
                                    <td>{{ row.created_at | fancy_datetime }}</td>
                                </tr>
                                <tr>
                                    <td>View count</td>
                                    <td>{{ row.view_count }}</td>
                                </tr>
                                <tr>
                                    <td>Last viewed</td>
                                    <td>
                                        {% if row.last_viewed %}
                                            {{ row.last_viewed | fancy_datetime }}
                                        {% else %}
                                            Never
                                        {% endif %}
                                    </td>
                                </tr>
                                <tr>
                                    <td>Collection name</td>
                                    <td>{{ row.collection_name }}</td>
                                </tr>
                            </table>
                            <p>
                                <a href="/file/{{ row.id_file }}/delete_scrapping" class="btn btn-danger">
                                    Delete scrapping
                                </a>
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


